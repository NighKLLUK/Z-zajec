import numpy as np
import matplotlib.pyplot as plt

x=np.arange(1,5,0.1)
plt.scatter(x,np.sin(x),color='green', marker='^',label='sin(x)')
plt.scatter(x,np.cos(x),color='red', marker='.',label='cos(x)')
plt.legend(loc='upper right')
plt.title('wykresy sin(x) i cos(x)')
plt.xlabel('x')
plt.ylabel('y')
plt.show()
